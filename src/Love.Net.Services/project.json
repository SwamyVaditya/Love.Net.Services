{
  "version": "1.0.0-*",
  "description": "This library contains some Asp.Net Core common services abstraction, such as SMS sender, Email sender and App message push and so on. The ApiInvoker as the default implementation by invoking RESTfull API.",
  "authors": [ "rigofunc", "rigofunc@outlook.com", "lotosbin", "lotosbin@gmail.com" ],
  "packOptions": {
    "projectUrl": "https://github.com/lovedotnet/Love.Net.Services",
    "licenseUrl": "",
    "repository": {
      "type": "git",
      "url": "https://github.com/lovedotnet/Love.Net.Services.git"
    },
    "tags": [ "SMS", "Email", "AppPush", "push", "aspnetcore" ]
  },
  "buildOptions": {
    "optimize": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ],
    "warningsAsErrors": true
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0",
    "Love.Net.Http": "1.0.0"
  },

  "frameworks": {
    "netstandard1.6": {
    }
  }
